{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Debug"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss"
    }
  },
  "AudioService": {
    "ProcessingIntervalSeconds": 10,
    "RetryDelaySeconds": 5,
    "MaxRetries": 2
  }
}
