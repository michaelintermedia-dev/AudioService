{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss"
    }
  },
  "AudioService": {
    "ProcessingIntervalSeconds": 30,
    "RetryDelaySeconds": 30,
    "MaxRetries": 3
  }
}
